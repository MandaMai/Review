import java.util.HashMap;
import java.util.Map;

public class ReviewItems {

	public static void main(String[] args) {

	//create new hashmap
	Map<Integer, String> testMap = new HashMap<>();
	
	//populate HashMap
	testMap.put(1, "One");
	testMap.put(2, "Two");
	testMap.put(3, "Three");
	testMap.put(4, "Four");
	
	testMap.putIfAbsent(8, "Eight");
	
	System.out.println(testMap);
	}

}
